hint max(hint a , hint b , hint c)[
    hif a >= b & a>=c [
        ret a; 
    ]

    hif b >= a & b>=c [
        ret b; 
    ] 

    ret c; 
]

hint fibo(hint n)[
    hif n==0 [
        ret 1; 
    ]

    hif n==1[
        ret 1; 
    ]

    ret fibo{n-1} + fibo{n-2}; 
]

hprintln fibo{8}; 