hint fact(hint n)[
    hif n == 1 [
        ret 1; 
    ]
    ret fact{n-1} * n; 
]

hint sum(hint n)[
    hif n == 0[ 
        ret 0; 
    ]
    ret sum{n-1} + n; 
]

hint ans = sum{fact{5}} + fact{5} + 4;

hint i = 0; 

hwhile i<=5 [
    hint j = 1; 
    hwhile j<=i[
        hprint j;  
        j = j + 1;
    ]
    hprintln j; 
    i = i + 1; 
]

